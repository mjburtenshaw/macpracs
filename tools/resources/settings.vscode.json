{
  "[Html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svg]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "Fira Code SemiBold",
  "editor.fontLigatures": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.bracketPairColorization.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git-graph.maxDepthOfRepoSearch": 1,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "python.defaultInterpreterPath": "/usr/local/bin/python",
  "terminal.integrated.fontFamily": "monospace",
  "workbench.sideBar.location": "right",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.alwaysShowStatus": true,
  "errorLens.enabled": true,
  "eslint.codeActionsOnSave.rules": null,
  "editor.stickyScroll.enabled": true,
  "editor.minimap.enabled": false,
  "git.autofetch": true,
  "javascript.suggest.autoImports": false,
  "javascript.suggest.includeCompletionsForImportStatements": false,
  "window.commandCenter": true,
  "workbench.layoutControl.enabled": false,
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "errorLens.gutterIconsEnabled": true,
  "workbench.startupEditor": "none",
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.blame.separateLines": false,
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "prettier.arrowParens": "avoid",
  "workbench.colorTheme": "Solarized Dark",
  "workbench.preferredDarkColorTheme": "Solarized Dark",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "go.toolsManagement.autoUpdate": true,
  "gitlens.graph.minimap.additionalTypes": ["localBranches"],
  "files.insertFinalNewline": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "redhat.telemetry.enabled": true,
  "errorLens.enabledDiagnosticLevels": ["info", "warning"],
  "editor.renderWhitespace": "none",
  "editor.tabSize": 2,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "git.ignoreMissingGitWarning": true,
  "[ruby]": {
    "editor.defaultFormatter": "jnbt.vscode-rufo"
  },
  "remote.autoForwardPortsSource": "hybrid",
